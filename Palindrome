#include <stdio.h>
int main() {
    int n, rev = 0, r;
    printf("Enter a number: ");
    scanf("%d", &n);
    int temp = n;  
    while (n > 0) {
        r = n % 10;       
        rev = rev * 10 + r;
        n = n / 10;       
    }
    if (temp == rev)
        printf("Palindrome number");
    else
        printf("Not a palindrome number");
    return 0;
}
