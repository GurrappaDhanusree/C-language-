#include <stdio.h>
#include <math.h>

int main() {
    int base, exponent;
   
    printf("Enter base: ");
    scanf("%d", &base);
   
    printf("Enter exponent: ");
    scanf("%d", &exponent);
   
    int result = pow(base, exponent); // Using math.h pow function to calculate power
   
    printf("%d^%d = %d\n", base, exponent, result);
   
    return 0;
}