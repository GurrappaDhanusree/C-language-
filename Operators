#include <stdio.h>
int main()
{
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);
    if(num > 0)
    {
        printf("The number is POSITIVE");
    }
    else if(num < 0)
    {
        printf("The number is NEGATIVE");
    }
    else
    {
        printf("The number is ZERO");
    }

    return 0;
}
